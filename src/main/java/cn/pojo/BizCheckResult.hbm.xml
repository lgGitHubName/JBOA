<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.pojo.BizCheckResult" table="biz_check_result" schema="jboa">
        <id name="id" column="ID"/>
        <property name="checkTime" column="CHECK_TIME"/>
        <property name="checkerSn" column="CHECKER_SN"/>
        <property name="result" column="RESULT"/>
        <property name="comm" column="COMM"/>
    </class>
</hibernate-mapping>